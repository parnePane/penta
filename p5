function setup() {
  createCanvas(1280, 800);
  background(220);
  angleMode(DEGREES);
}

function draw() {
  translate(640,400);
  noFill();
  scale(1,-1);
  ellipse(circle.x, circle.y, circle.r, circle.r);
  poly(0,0,30,60);
  poly(0,0,90,30);
  poly(0,0,180,15);
}

function poly(x,y,r,angle) {
	for (var a = 0; a < 360; a += angle) {
    x = r * sin(a);
    y = r * cos(a);
    point(x,y,);
  }
}
