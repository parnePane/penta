

function setup() {
  createCanvas(400, 400);
}

function draw() {
  translate(200,200);
  background(0);
  rotate(PI/2);
  // ellipse(0,0,120);
  polyDots(25,90);
}

function polyDots(i,r) {
  noFill();
  stroke(200);
  strokeWeight(1);
  for (angle = 0; angle < TWO_PI; angle += PI/i) {
    let radius = r;
    let dx = radius/2 * cos(angle);
    let dy = radius/2 * sin(angle);
    ellipse(dx,dy,r);
  }
}
